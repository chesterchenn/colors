{
  "name": "colors-api",
  "version": "0.0.1",
  "description": "This is colors api",
  "main": "index.js",
  "scripts": {
    "eslint": "npx eslint src test",
    "eslint:fix": "npx eslint src test --fix",
    "start:dev": "node src/index",
    "start": "nodemon --exec babel-node -- src/index",
    "test": "set NODE_ENV=test && npx mocha test/routes/*.spec.js --exit --timeout 10000",
    "test:cov": "set NODE_ENV=test && nyc --reporter=html mocha test/routes/*.spec.js --exit --timeout 10000"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "mariadb": "^2.1.0",
    "sequelize": "^5.10.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/node": "^7.5.0",
    "@babel/preset-env": "^7.5.4",
    "chai": "^4.2.0",
    "eslint": "^6.0.1",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "supertest": "^4.0.2"
  },
  "keywords": [
    "color",
    "express",
    "node"
  ],
  "author": "chesterchenn",
  "repository": {
    "url": "git@github.com:chesterchenn/colors-api.git"
